{
  "name": "server",
  "version": "1.0.0",
  "description": "The Totem server.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "npm run init-db-dev && nodemon index.js",
    "init-db-prod": "mkdir -p db && node tools/db_init.js prod",
    "init-db-dev": "mkdir -p db && rm -f db/dev* && node tools/db_init.js dev"
  },
  "author": "Todd Griffin",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.3",
    "node-emoji": "^1.8.1",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
